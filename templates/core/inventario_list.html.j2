{% extends "core/base.html" %}
{% block content %}
<h2>Inventario</h2>

<table border="1">
<tr>
    <th>ID</th>
    <th>Nombre</th>
    <th>Stock</th>
    <th>Proveedor</th>
</tr>

{% for i in items %}
<tr>
<td>{{ i.item_id }}</td>
<td>{{ i.nombre }}</td>
<td>{{ i.stock_actual }}</td>
<td>{{ i.proveedor }}</td>
</tr>
{% endfor %}
</table>

{% endblock %}
